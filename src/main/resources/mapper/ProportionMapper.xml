<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper  namespace="com.winterchen.dao.ProportionDao">
    <update id="update" parameterType="com.winterchen.model.ProportionDomain"  >
       update w_proportion set teacher=#{teacher},director=#{director},pt=#{pt} where id = 1
    </update>

    <select id="list" resultType="com.winterchen.model.ProportionDomain">
        select * from w_proportion where id = 1
    </select>
</mapper>